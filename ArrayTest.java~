public class ArrayTest {
  
  public static void main( String[] args) {
   
    int[] c = new int[ 10 ];
    int j = 1;
    
    for( int i = 0; i <  c.length(); i++ ) {
      c[i] = j;
      j += 2;
      System.out.println( c[i] );
    }
    
    
    
    
  } // end main()
  
} // end class ArrayTest